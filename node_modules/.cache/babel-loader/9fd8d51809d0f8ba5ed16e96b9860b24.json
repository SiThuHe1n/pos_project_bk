ion v-for=\"c in subcategory\" :value=c.id>{{c.name}}</option>\r\n                           \r\n                            </select>\r\n        \r\n                        </div>\r\n\r\n                  \r\n                      \r\n                    </div>\r\n               \r\n                    <!-- unittype -->\r\n                    <label for=\"\">Unit Type</label> \r\n                    <div class=\"row gx-0\">\r\n                        <div class=\"col-sm-12\">\r\n                            <select  name=\"unit\" class=\"form-select \" v-model=\"mtypelist\" id=\"mtypelist\">\r\n                          <option value=\"\">hello</option>\r\n                          <option value=\"\">hello</option>\r\n                            <option v-for=\"c in mtype\" :value=c.id>{{c.name}}</option>\r\n                           \r\n                            </select>\r\n        \r\n                        </div>\r\n\r\n                      \r\n                    </div>\r\n                \r\n\r\n\r\n             \r\n\r\n                    <label for=\"\">Purchase Price</label>\r\n                    <div class=\"row gx-0 \">\r\n                        <div class=\"col-sm-12\"> \r\n                            <input type=\"text\" name=\"price\" class=\"form-control\" v-model=\"purchaseprice\">\r\n                        </div>\r\n\r\n                    \r\n                                                \r\n                     </div>\r\n\r\n                     <label for=\"\">Amount </label>\r\n                    <div class=\"row gx-0 \">\r\n                        <div class=\"col-sm-12\"> \r\n                            <input type=\"text\" name=\"amount\" class=\"form-control\" v-model=\"amount\">\r\n                        </div>\r\n\r\n                    \r\n                                                \r\n                     </div>\r\n          \r\n            \r\n                    \r\n           \r\n\r\n\r\n       \r\n\r\n\r\n\r\n\r\n\r\