{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      link: 'http://127.0.0.1:8000',\n      percentage: '',\n      saleprice: '',\n      purchaseprice: '',\n      brandlist: '',\n      categorylist: '',\n      subcategorylist: '',\n      mtypelist: '',\n      brand: [],\n      category: [],\n      subcategory: [],\n      mtype: [],\n      newbrand: '',\n      newcategory: '',\n      newsubcategory: '',\n      newmtype: '',\n      isnewbrand: false,\n      isnewcategory: false,\n      isnewsubcategory: false,\n      isnewmtype: false,\n      bload: false,\n      scload: false,\n      cload: false,\n      mload: false,\n      isaddproduct: false\n    };\n  },\n\n  mounted() {\n    this.showbrand();\n  },\n\n  methods: {\n    setpercentage() {\n      if (this.saleprice != \"\") {\n        let value = parseInt(this.saleprice) - parseInt(this.purchaseprice);\n        this.percentage = parseFloat(value / this.purchaseprice * 100);\n      }\n    },\n\n    calpercentage() {\n      if (this.purchaseprice != \"\") {\n        let saleprice = 0;\n        saleprice = parseInt(this.purchaseprice) + parseInt(this.purchaseprice / 100 * this.percentage);\n        this.saleprice = saleprice;\n      }\n    },\n\n    dodelete: function (database, setdata, loading, inputform) {\n      axios.delete(this.link + \"/api/\" + database + \"/\" + this.inputform).then(response => console.log(response)).then(this.doshow(database, setdata, loading, inputform), this.doshow(database, setdata, loading, inputform)).catch(error => {\n        this.errorMessage = error.message;\n        console.error(\"There was an error!\", error);\n      });\n    },\n    doshow: function (database, setdata, loading, inputform) {\n      axios.get(this.link + \"/api/\" + database).then(response => {\n        setdata = response.data;\n      }).catch(error => {\n        this.errorMessage = error.message;\n        console.error(\"There was an error!\", error);\n      });\n    },\n    doadd: function (database, setdata, loading, inputform) {\n      if (this.newbrand == \"\") {\n        return document.getElementById(\"newbrand\").focus();\n      }\n\n      loading = true;\n      let article = {\n        data1: this.newbrand\n      };\n      axios.post(this.link + \"/api/\" + database, article).then(response => {\n        console.log(response);\n        this.bload = false;\n      }).catch(error => {\n        this.errorMessage = error.message;\n        console.error(\"There was an error!\", error);\n      });\n      this.doshow(database, setdata);\n      this.doshow(database, setdata);\n      this.doshow(database, setdata);\n      this.doshow(database, setdata);\n    },\n\n    deletecategory() {\n      axios.delete(this.link + \"/api/category/\" + this.categorylist).then(response => console.log(response)).then(this.showbrand(), this.showbrand()).catch(error => {\n        this.errorMessage = error.message;\n        console.error(\"There was an error!\", error);\n      });\n    },\n\n    showcategory() {\n      axios.get(this.link + \"/api/category\").then(response => {\n        this.category = response.data;\n      }).catch(error => {\n        this.errorMessage = error.message;\n        console.error(\"There was an error!\", error);\n      });\n    },\n\n    addcategory() {\n      if (this.newcategory == \"\") {\n        return document.getElementById(\"newcategory\").focus();\n      }\n\n      this.cload = true;\n      let article = {\n        data1: this.newcategory\n      };\n      axios.post(this.link + \"/api/category\", article).then(response => {\n        console.log(response);\n        this.cload = false;\n      }).catch(error => {\n        this.errorMessage = error.message;\n        console.error(\"There was an error!\", error);\n      });\n      this.showcategory();\n      this.showcategory();\n      this.showcategory();\n      this.showcategory();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyVA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAA