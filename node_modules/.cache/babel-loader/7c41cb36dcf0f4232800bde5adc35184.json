;\n      }\n\n      if (database == 'category') {\n        this.cload = true;\n      }\n\n      if (database == 'subcategory') {\n        this.scload = true;\n      }\n\n      if (database == 'unittype') {\n        this.mload = true;\n      }\n\n      let article = {\n        data1: v_model\n      };\n      axios.post(this.link + \"/api/\" + database, article).then(response => {\n        console.log(response);\n\n        if (database == 'brand') {\n          this.bload = false;\n        }\n\n        if (database == 'category') {\n          this.cload = false;\n        }\n\n        if (database == 'subcategory') {\n          this.scload = false;\n        }\n\n        if (database == 'unittype') {\n          this.mload = false;\n        }\n      }).catch(error => {\n        this.errorMessage = error.message;\n        console.error(\"There was an error!\", error);\n      });\n      this.doshow(database);\n      this.doshow(database);\n      this.doshow(database);\n      this.doshow(database);\n    }\n  }\n};","map":{"version":3,"mappings":"AAuWA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,IAAG,GAAK;IAGJ,OAAO;MACHC,IAAI,EAAC,uBADF;MAEHC,UAAU,EAAC,EAFR;MAGHC,SAAS,EAAC,EAHP;MAIHC,aAAa,EAAC,EAJX;MAKHC,SAAS,EAAC,EALP;MAMHC,YAAY,EAAC,EANV;MAOJC,eAAe,EAAC,EAPZ;MAQHC,SAAS,EAAC,EARP;MAUHC,SAAS,EAAC,EAVP;MAYHC,QAAQ,EAAC,EAZN;MAaHC,WAAW,EAAC,EAbT;MAcHC,KAAK,EAAC,EAdH;MAeHC,QAAQ,EAAC,EAfN;MAgBHC,WAAW,EAAC,EAhBT;MAiBHC,cAAc,EAAC,EAjBZ;MAkBHC,QAAQ,EAAC,EAlBN;MAoBHC,YAAY,EAAC;IApBV,CAAP;EAuBH,CA3BU;;EA4BXC,OAAM,GAAK;IACP,KAAKC,MAAL,CAAY,OAAZ;IACA,KAAKA,MAAL,CAAY,UAAZ;IACA,KAAKA,MAAL,CAAY,aAAZ;IACA,KAAKA,MAAL,CAAY,UAAZ;EAEH,CAlCU;;EAmCXC,OAAO,EAAE;IACLC,aAAa,GACb;MACI,IAAG,KAAKlB,SAAL,IAAgB,EAAnB,EACA;QACA,IAAImB,KAAK,GAACC,QAAQ,CAAC,KAAKpB,SAAN,CAAR,GAAyBoB,QAAQ,CAAC,KAAKnB,aAAN,CAA3C;QACA,KAAKF,UAAL,GAAgBsB,UAAU,CAACF,KAAK,GAAC,KAAKlB,aAAX,GAAyB,GAA1B,CAA1B;MACA;IAEH,CATI;;IAULqB,aAAa,GAAG;MACZ,IAAG,KAAKrB,aAAL,IAAoB,EAAvB,EACA;QACI,IAAID,SAAS,GAAC,CAAd;QACJA,SAAS,GAAEoB,QAAQ,CAAC,KAAKnB