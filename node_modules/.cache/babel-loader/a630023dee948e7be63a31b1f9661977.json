ere was an error!\", error);\n      });\n    },\n    doadd: function (database, v_model, inputform) {\n      if (v_model == \"\") {\n        return document.getElementById(inputform).focus();\n      }\n\n      if (database = 'brand') {\n        this.bload = true;\n      }\n\n      if (database = 'category') {\n        this.cload = true;\n      }\n\n      let article = {\n        data1: v_model\n      };\n      axios.post(this.link + \"/api/\" + database, article).then(response => {\n        console.log(response);\n\n        if (database = 'brand') {\n          this.bload = false;\n        }\n\n        if (database = 'category') {\n          this.cload = false;\n        }\n      }).catch(error => {\n        this.errorMessage = error.message;\n        console.error(\"There was an error!\", error);\n      });\n      this.doshow(database);\n      this.doshow(database);\n      this.doshow(database);\n      this.doshow(database);\n    }\n  }\n};","map":{"version":3,"mappings":"AAkWA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,IAAG,GAAK;IAGJ,OAAO;MACHC,IAAI,EAAC,uBADF;MAEHC,UAAU,EAAC,EAFR;MAGHC,SAAS,EAAC,EAHP;MAIHC,aAAa,EAAC,EAJX;MAKHC,SAAS,EAAC,EALP;MAMHC,YAAY,EAAC,EANV;MAOJC,eAAe,EAAC,EAPZ;MAQHC,SAAS,EAAC,EARP;MAUHC,SAAS,EAAC,EAVP;MAYHC,QAAQ,EAAC,EAZN;MAaHC,WAAW,EAAC,EAbT;MAcHC,KAAK,EAAC,EAdH;MAeHC,QAAQ,EAAC,EAfN;MAgBHC,WAAW,EAAC,EAhBT;MAiBHC,cAAc,EAAC,EAjBZ;MAkBHC,QAAQ,EAAC,EAlBN;MAmBHC,UAAU,EAAC,KAnBR;MAoBHC,aAAa,EAAC,KApBX;MAqBHC,gBAAgB,EAAC,KArBd;MAsBHC,UAAU,EAAC,KAtBR;MAuBHC,KAAK,EAAC,KAvBH;MAwBHC,MAAM,EAAC,KAxBJ;MAyBHC,KAAK,EAAC,KAzBH;MA0BHC,KAAK,EAAC,KA1BH;MA2BHC,YAAY,EAAC;IA3BV,CAAP;EA8BH,CAlCU;;EAmCXC,OAAM,GAAK;IACP,KAAKC,MAAL,CAAY,OAAZ;EAEH,CAtCU;;EAuCXC,OAAO,EAAE;IACLC,aAAa,GACb;MACI,IAAG,KAAK1B,SAAL,IAAgB,EAAnB,EACA;QACA,IAAI2B,KAAK,GAACC,QAAQ,CAAC,KAAK5B,SAAN,CAAR,GAAyB4B,QAAQ,CAAC,KAAK3B,aAAN,CAA3C;QACA,KAAKF,UAAL,GAAgB8B,UAAU,CAACF,KAAK,GAAC,KAAK1B,aAAX,GAAyB,GAA1B,CAA1B;MACA;IAEH,CATI;;IAUL6B,aAAa,GAAG;MACZ,IAAG,KAAK7B,aAAL,IAAoB,EAAvB,EACA;QACI,IAAID,SAAS,GAA