ents","axios","name","data","purchasdate","Date","toISOString","slice","totalpaid","supplier","showproduct","showproduct2","selectedproduct","purchaseList","sprice","pprice","qty","mtype","desc","id","btnproduct","totalprice","search","pid","productlist","percentage","selectpick","stocklist","methods","showdata","get","localStorage","getItem","$refs","form","then","response","a","length","b","mtypelist","code","unittype_id","push","console","log","catch","error","errorMessage","message","docreate","post","doshow","database","listbrand","category","subcategory","doshowproduct","setpercentage","value","parseInt","parseFloat","calpercentage","saleprice","caltotalprice","totalprc","addproduct","document","getElementById","focus","alert","removeproduct","idas","splice","edit","selectitem","searcher","toLowerCase","Brand","Category","SubCategory","setproduct","printvoucher","mywindow","window","open","write","title","innerHTML","close","print","mounted","$","selectpicker","liveSearch","noneSelectedText","updated","watch","filterstate"],"sourceRoot":"","sources":["/Users/sithuhein/Desktop/PosSystem/pos_project_bk/src/views/Purchase/purchaseItem.vue"],"sourcesContent":["<template>\n    <div>\n        <section>\n            <div class=\"row gx-0\">\n                <div class=\"col-sm-5\">\n                    <div class=\"row gx-0\">\n                     \n                        <div class=\"col-md-9\">\n        \n                            <select name=\"\" class=\" selectpick\" data-live-search=\"true\" v-model=\"selectpick\"  id=\"selectpick\">\n                                <option value=\"\"> Select </option>\n                                <option v-for=\"pl in productlist\" :value=\" pl.id \"> {{ pl.code  }} {{pl.name}} </option>\n                             \n                            </select>\n\n                     \n                        </div>\n                        <div class=\"col-md-2\">\n                            <button class=\"btn btn-primary\" @click=\"setproduct()\">Add</button>\n                        </div>\n                    </div>\n\n                    <div class=\"row\">\n                        <div class=\"col-md-12\">\n                            <div class=\"card p-3 m-3 ms-4 shadow  bg-body rounded\">\n                                <div class=\"row gx-0 mb-3\">\n                        <div class=\"col-md-5\">\n                           <label for=\"\">Product Name : </label>\n                           \n                        </div>\n                        <div class=\"col-md-7\">\n                            <input type=\"text\" class=\"form-control\" id=\"name\" v-model=\"name\" disabled>\n                        </div>\n                    </div>\n                    <div class=\"row gx-0 mb-3\">\n                        <div class=\"col-md-5\">\n                           <label for=\"\">Purchase Price : </label>\n                        </div>\n                        <div class=\"col-md-7\">\n                            <input type=\"number\" class=\"form-control\" id=\"pprice\"   v-model=\"pprice\">\n                        </div>\n                    </div>\n                 \n                    <div class=\"row gx-0 mb-3\">\n                        <div class=\"col-md-4\">\n                           <label for=\"\">Quantity : </label>\n                        </div>\n                        <div class=\"col-md-5\">\n                            <input type=\"number\" class=\"form-control\" id=\"qty\" v-model=\"qty\">\n                        </div>\n                        <div class=\"col-md-3\">\n                         <select name=\"\" class=\"form-control\"   id=\"mtype\" v-model=\"mtype\">\n                            <option value=\"\"> Unit</option>\n                            <option v-for=\"sl in stocklist\" :value=\"sl.code\">\n\n                               \n                                {{ sl.name }}\n                              \n                               </option>\n                           \n                         </select>\n                         \n                        \n\n                        </div>\n                    </div>\n                    <div class=\"row gx-0 mb-3 \">\n                        <div class=\"col-md-4\">\n                           <label for=\"\">Note : </label>\n                        </div>\n                        <div class=\"col-md-8\">\n                            <textarea type=\"text\" class=\"form-control\" v-model=\"desc\"></textarea>\n                        </div>\n                    </div>\n                    <div class=\"row gx-0 mb-3 d-flex justify-content-center \">\n                    <div class=\"col-md-8\">\n                        <button class=\"btn btn-primary\" @click=addproduct()>{{btnproduct}}</button>\n                    </div>\n                    </div>\n                            </div>\n                        </div>\n                    </div>\n               \n                </div>\n                 <div class=\"col-sm-7\">\n                    <form action=\"\"  ref=\"form\" v-on:submit.prevent=\"docreate\">\n             \n                    <div class=\"row  card p-3 m-2 me-4 mb-3 shadow  bg-body rounded\">\n                        <div class=\"col-md-12\">\n                            <div class=\"row \">\n                                <div class=\"col-md-6\">\n                        <label for=\"\">Supplier</label>   \n                        <select name=\"supplier\" class=\"form-control\" v-model=\"supplier\" id=\"\">\n                        <option value=\"Ko Aung\">Ko Aung</option>\n                        <option value=\"Ko Aung\">Ko Myo</option>\n                        <option value=\"Ko Aung\">Ko Soe</option>\n                        <option value=\"Ko Aung\">Ko Thu</option>\n                    \n                    \n                    </select>\n\n\n                    \n                        <label for=\"\">Total Price</label>   \n                        <input type=\"text\" v-model=\"totalprice\" class=\"form-control\" disabled>\n                        <input type=\"hidden\" name=\"totalprice\" v-model=\"totalprice\" class=\"form-control\" >\n                        </div>\n                        <div class=\"col-md-6\">\n                            <label for=\"\">Total Paid</label>   \n                        <input type=\"text\" class=\"form-control\" name=\"totalpaid\" v-model=\"totalpaid\">\n                       \n                            <label for=\"\">Purchase Date</label>   \n                        <input type=\"date\" name=\"purchasdate\" v-model=\"purchasdate\"  class=\"form-control\" >\n                        <br>\n                        <button class=\"btn btn-primary\" type=\"submit\" @click=\"printvoucher\">doprint</button>\n                        <button class=\"btn btn-primary\" type=\"submit\" @click=\"\">Purchase</button>\n                        \n                        </div>\n                            </div>\n                        </div>\n                    \n                    </div>\n                    <div class=\"row card p-3 m-2 me-4 shadow  bg-body rounded\">\n                        <div class=\"col-md-12 overflow-scroll\">\n                            <input type=\"text\" :value=\"JSON.stringify(purchaseList)\" name=\"productdata\">\n                            <table class=\"table table-bordered \">\n                            <thead>\n                                <tr>\n                                    <th>No</th>\n                                    <th>Name</th>\n                                    <th>Qty</th>\n                                    <th>Purchase_Price</th>\n                                    <th>Note</th>\n                                    <th>Action</th>\n                                </tr>\n\n                            </thead>\n                            <tbody>\n               