          scload:false,\r\n            cload:false,\r\n            mload:false,\r\n            isaddproduct:false,\r\n\r\n        }\r\n    },\r\n    mounted () {\r\n        this.doshow('brand',this.listbrand)\r\n    \r\n    },\r\n    methods: {\r\n        setpercentage()\r\n        {\r\n            if(this.saleprice!=\"\")\r\n            {\r\n            let value=parseInt(this.saleprice)-parseInt(this.purchaseprice);\r\n            this.percentage=parseFloat(value/this.purchaseprice*100);\r\n            }\r\n        \r\n        },\r\n        calpercentage() {\r\n            if(this.purchaseprice!=\"\")\r\n            {\r\n                let saleprice=0;\r\n            saleprice= parseInt(this.purchaseprice) +parseInt((this.purchaseprice/100)*this.percentage);\r\n            this.saleprice=saleprice;\r\n            }\r\n \r\n\r\n        },\r\n        \r\n   dodelete:function(database,setdata,inputform){\r\n\r\naxios.delete(this.link+\"/api/\"+database+\"/\"+inputform)\r\n  .then(response =>console.log(response)).then(\r\n \r\n      this.doshow(database,setdata),\r\n       this.doshow(database,setdata)\r\n       \r\n\r\n\r\n\r\n\r\n\r\n  )\r\n  .catch(error => {\r\n    this.errorMessage = error.message;\r\n    console.error(\"There was an error!\", error);\r\n     })\r\n\r\n},\r\ndoshow:function(database,setstoredata){\r\n    console.log(setstoredata)\r\n    let getstoredata=[];\r\n    axios.get(this.link+\"/api/\"+database)\r\n       .then(response => {\r\n        getstoredata = response.data,\r\n        console.log(setstoredata)\r\n            console.log(getstoredata)\r\n            setstoredata=getstoredata\r\n           \r\n       })\r\n       .catch(error => {\r\n         this.errorMessage = error.message;\r\n         console.error(\"There was an error!\", error);});\r\n         \r\n\r\n           },\r\ndoadd:function(database,setdata,doload,v_model,inputform){\r\n          \r\n        \r\n         \r\n               if(v_model==\"\")\r\n               {\r\n                 return  document.getElementById(inputform).focus();\r\n               }\r\n              \r\n               doload=true\r\n               let article = \r\n               {\r\n                 data1:v_model,\r\n         \r\n              \r\n   \r\n               };\r\n               \r\n     axios.post(this.link+\"/api/\"+database, article)\r\n       .then(response =>{console.log(response)\r\n       \r\n        doload=false\r\n       })\r\n       .catch(error => {\r\n         this.errorMessage = error.message;\r\n         console.error(\"There was an error!\", error);\r\n          })\r\n          this.doshow(database,setdata);\r\n          this.doshow(database,setdata);\r\n          this.doshow(database,setdata);\r\n          this.doshow(database,setdata);\r\n        \r\n     \r\n          \r\n      \r\n  \r\n   \r\n   },\r\n\r\n\r\n   deletecategory(){\r\n\r\n       \r\n   \r\n\r\naxios.delete(this.link+\"/api/category/\"+this.categorylist)\r\n  .then(response =>console.log(response)).then(\r\n \r\n      this.doshow(database,setdata),\r\n       this.showbrand()\r\n       \r\n\r\n\r\n\r\n\r\n\r\n  )\r\n  .catch(error => {\r\n    this.errorMessage = error.message;\r\n    console.error(\"There was an error!\", error);\r\n     })\r\n\r\n},\r\nshowcategory(){\r\n    \r\n    axios.get(this.link+\"/api/category\")\r\n       .then(response => {\r\n               this.category = response.data;\r\n         \r\n               \r\n       })\r\n       .catch(error => {\r\n         this.errorMessage = error.message;\r\n         console.error(\"There was an error!\", error);});\r\n           },\r\naddcategory(){\r\n          \r\n        \r\n         \r\n               if(this.newcategory==\"\")\r\n               {\r\n                 return  document.getElementById(\"newcategory\").focus();\r\n               }\r\n              \r\n               this.cload=true\r\n               let article = \r\n               {\r\n                 data1:this.newcategory,\r\n         \r\n              \r\n   \r\n               };\r\n               \r\n     axios.post(this.link+\"/api/category\", article)\r\n       .then(response =>{console.log(response)\r\n       \r\n        this.cload=false\r\n       })\r\n       .catch(error => {\r\n         this.errorMessage = error.message;\r\n         console.error(\"There was an error!\", error);\r\n          })\r\n          this.showcategory();\r\n          this.showcategory();\r\n          this.showcategory();\r\n          this.showcategory();\r\n        \r\n     \r\n          \r\n      \r\n  \r\n   \r\n   },\r\n    },\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n    ._forminput{\r\n        min-width:350px;\r\n     \r\n    }\r\n</style>"]},"metadata":{},"sourceType":"module"}