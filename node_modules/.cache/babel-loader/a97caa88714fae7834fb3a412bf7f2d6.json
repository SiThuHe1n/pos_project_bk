 => {\n        this.errorMessage = error.message;\n        console.error(\"There was an error!\", error);\n      });\n    },\n    doadd: function (database, setdata, doload, v_model, inputform) {\n      if (v_model == \"\") {\n        return document.getElementById(inputform).focus();\n      }\n\n      doload = true;\n      let article = {\n        data1: v_model\n      };\n      axios.post(this.link + \"/api/\" + database, article).then(response => {\n        console.log(response);\n        doload = false;\n      }).catch(error => {\n        this.errorMessage = error.message;\n        console.error(\"There was an error!\", error);\n      });\n      this.doshow(database, setdata);\n      this.doshow(database, setdata);\n      this.doshow(database, setdata);\n      this.doshow(database, setdata);\n    },\n\n    deletecategory() {\n      axios.delete(this.link + \"/api/category/\" + this.categorylist).then(response => console.log(response)).then(this.doshow(database, setdata), this.showbrand()).catch(error => {\n        this.errorMessage = error.message;\n        console.error(\"There was an error!\", error);\n      });\n    },\n\n    showcategory() {\n      axios.get(this.link + \"/api/category\").then(response => {\n        this.category = response.data;\n      }).catch(error => {\n        this.errorMessage = error.message;\n        console.error(\"There was an error!\", error);\n      });\n    },\n\n    addcategory() {\n      if (this.newcategory == \"\") {\n        return document.getElementById(\"newcategory\").focus();\n      }\n\n      this.cload = true;\n      let article = {\n        data1: this.newcategory\n      };\n      axios.post(this.link + \"/api/category\", article).then(response => {\n        console.log(response);\n        this.cload = false;\n      }).catch(error => {\n        this.errorMessage = error.message;\n        console.error(\"There was an error!\", error);\n      });\n      this.showcategory();\n      this.showcategory();\n      this.showcategory();\n      this.showcategory();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyVA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,IAAG,GAAK;IAGJ,OAAO;MACHC,IAAI,EAAC,uBADF;MAEHC,UAAU,EAAC,EAFR;MAGHC,SAAS,EAAC,EAHP;MAIHC,aAAa,EAAC,EAJX;MAKHC,SAAS,EAAC,EALP;MAMHC,YAAY,EAAC,EANV;MAOJC,eAAe,EAAC,EAPZ;MAQHC,SAAS,EAAC,EARP;MAUHC,SAAS,EAAC,EAVP;MAYHC,QAAQ,EAAC,EAZN;MAaHC,WAAW,EAAC,EAbT;MAcHC,KAAK,EAAC,EAdH;MAeHC,QAAQ,EAAC,EAfN;MAgBHC,WAAW,EAAC,EAhBT;MAiBHC,cAAc,EAAC,EAjBZ;MAkBHC,QAAQ,EAAC,EAlBN;MAmBHC,UAAU,EAAC,KAnBR;MAoBHC,aAAa,EAAC,KApBX;MAqBHC,gBAAgB,EAAC,KArBd;MAsBHC,UAAU,EAAC,KAtBR;MAuBHC,KAAK,EAAC,KAvBH;MAwBHC,MAAM,EAAC,KAxBJ;MAyBHC,KAAK,EAAC,KAzBH;MA0BHC,KAAK,EAAC,KA1BH;MA2BHC,YAAY,EAAC;IA3BV,CAAP;EA8BH,CAlCU;;EAmCXC,OAAM,GAAK;IACP,KAAKC,MAAL,CAAY,OAAZ,EAAoB,KAAKlB,SAAzB;EAEH,CAtCU;;EAuCXmB,OAAO,EAAE;IACLC,aAAa,GACb;MACI,IAAG,KAAK1B,SAAL,IAAgB,EAAnB,EACA;QACA,IAAI2B,KAAK,GAACC,QAAQ,CAAC,KAAK5B,SAAN,CAAR,GAAyB4B,QAAQ,CAAC,KAAK3B,aAAN,CAA3C;QACA,KAAKF,UAAL,GAAgB8B,UAAU,CAACF,KAAK,GAAC,KAAK1B,aAAX,GAAyB,GAA1B,CAA1B;MACA;IAEH,CATI;;IAUL6B,aAAa,GAAG;MACZ,IAAG,KAAK7B,aAAL,IAAoB,EAAvB,EACA;QACI,IAAID,SAAS,GAAC,CAAd;QACJA,SAAS,GAAE4B,QAAQ,CAAC,KAAK3B,aAAN,CAAR,GAA8B2B,QAAQ,CAAE,KAAK3B,aAAL,GAAmB,GAApB,GAAyB,KAAKF,UAA/B,CAAjD;QACA,KAAKC,SAAL,GAAeA,SAAf;MACA;IAGH,CAnBI;;IAqBV+B,QAAQ,EAAC,UAASC,QAAT,EAAkBC,OAAlB,EAA0BC,SAA1B,EAAoC;MAEhDtC,KAAK,CAACuC,MAAN,CAAa,KAAKrC,IAAL,GAAU,OAAV,GAAkBkC,QAAlB,GAA2B,GAA3B,GAA+BE,SAA5C,EACGE,IADH,CACQC,QAAO,IAAIC,OAAO,CAACC,GAAR,CAAYF,QAAZ,CADnB,EAC0CD,IAD1C,CAGM,KAAKZ,MAAL,CAAYQ,QAAZ,EAAqBC,OAArB,CAHN,EAIO,KAAKT,MAAL,CAAYQ,QAAZ,EAAqBC,OAArB,CAJP,EAYGO,KAZH,CAYSC,KAAI,IAAK;QACd,KAAKC,YAAL,GAAoBD,KAAK,CAACE,OAA1B;QACAL,OAAO,CAACG,KAAR,CAAc,qBAAd,EAAqCA,KAArC;MACE,CAfN;IAiBC,CAxCY;IAyCbjB,MAAM,EAAC,UAASQ,QAAT,EAAkBY,YAAlB,EAA+B;MAClCN,OAAO,CAACC,GAAR,CAAYK,YAAZ;MACA,IAAIC,YAAY,GAAC,EAAjB;MACAjD,KAAK,CAACkD,GAAN,CAAU,KAAKhD,IAAL,GAAU,OAAV,GAAkBkC,QAA5B,EACII,IADJ,CACSC,QAAO,IAAK;QACjBQ,YAAW,GAAIR,QAAQ,CAACxC,IAAxB,EACAyC,OAAO,CAACC,GAAR,CAA